{
  "name": "electron-kt-advance",
  "version": "v0.2.1-alpha",
  "description": "no descr. by now",
  "license": "MIT",
  "author": {
    "name": "Artem Zaborskii",
    "email": "artem@toptal.com",
    "url": "http://zaborskiy.org"
  },
  "main": "app/main.js",
  "scripts": {
    "postinstall": "install-app-deps && node_modules/.bin/typings install && node_modules/.bin/bower install",
    "__compile": "node_modules/.bin/node-sass src/main/sass/styles.scss app/css/styles.css && node_modules/.bin/tsc",
    "__start": "npm install && npm run compile && node_modules/.bin/electron .",
    "pack": "build --dir",
    "__dist": "npm run compile && build",
    "build": "gulp build",
    "prerelease": "gulp build --env=production",
    "release": "build",
    "pretest": "gulp build-unit --env=test",
    "test": "node_modules/.bin/electron-mocha --renderer -r source-map-support/register -r mocha-typescript --compilers ts:ts-node/register ./src/modules/**/*.spec.ts",
    "pree2e": "gulp build-e2e --env=test",
    "e2e": "mocha app/e2e.js.autogenerated --require source-map-support/register"
  },
  "devDependencies": {
    "@types/es6-promise": "0.0.32",
    "@types/graphlib": "^2.1.3",
    "@types/lodash": "^3.10.1",
    "@types/mocha": "^2.2.41",
    "@types/node": "^7.0.7",
    "bower": "^1.8.0",
    "chai": "^4.0.2",
    "del": "^3.0.0",
    "electron": "^1.8.4",
    "electron-builder": "20.11.0",
    "electron-mocha": "^3.4.0",
    "fcopy-pre-bundled": "^0.3.4",
    "fs-jetpack": "^1.0.0",
    "gulp": "^3.9.1",
    "gulp-batch": "^1.0.5",
    "gulp-inject": "^4.2.0",
    "gulp-plumber": "^1.2.0",
    "gulp-sass": "^4.0.1",
    "gulp-typescript": "^4.0.1",
    "gulp-vulcanize": "^7.0.0",
    "gulp-watch": "^4.3.11",
    "minimist": "^1.2.0",
    "mocha": "^3.4.2",
    "mocha-typescript": "^1.1.4",
    "node-sass": "^4.8.3",
    "numeral": "^1.5.3",
    "rollup": "^0.42.0",
    "rollup-plugin-istanbul": "^1.1.0",
    "run-sequence": "^1.2.2",
    "ts-node": "^3.0.6",
    "typescript": "^2.2.1",
    "typings": "1.4.0",
    "hoek": "^5.0.3"
  },
  "dependencies": {
    "async": "^1.4.2",
    "d3": "^3.5.12",
    "dagre": "^0.7.4",
    "expand-home-dir": "0.0.3",
    "find-java-home": "^0.2.0",
    "graphlib": "^1.0.7",
    "jquery": "^3.0.0",
    "lodash": "^3.10.1",
    "path-exists": "^3.0.0",
    "typeface-roboto-mono": "0.0.54",
    "vscode-languageserver-protocol": "^3.7.1"
  },
  "build": {
    "appId": "kt.advance.gui",
    "asarUnpack": [
      "**/*.jar"
    ],
    "files": [
      "java/kt-advance-xml-2.3-jar-with-dependencies.jar",
      "app/**/*",
      "node_modules/**/*",
      "package.json"
    ],
    "mac": {
      "category": "public.app-category.developer-tools"
    }
  }
}
